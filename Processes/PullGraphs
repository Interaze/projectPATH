<?php
    session_start();
    if($_SESSION['UserName'] != null){
        $dir = "/Users/"+$_SESSION['UserName'];
        // Sort in ascending order - this is default
        $a = scandir($dir);
        array_shift($a);
        array_shift($a);
        array_unshift($a, "1");

        echo json_encode($a);
    }
    else{
        echo json_encode([
            'Error' => '0',
            'isSignedIn' => 'false'
        ]);
    }

?>
